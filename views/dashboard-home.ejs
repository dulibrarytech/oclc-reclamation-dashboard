<%- include('partials/header.ejs'); %>
<body>
<!-- Left Panel -->
<aside id="left-panel" class="left-panel">
    <nav class="navbar navbar-expand-sm navbar-default">
        <div id="main-menu" class="main-menu collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li class="active">
                    <a href="/dashboard/home"><i class="menu-icon fa fa-laptop"></i>OCLC Reclamation </a>
                </li>
                <li class="menu-title">Menu</li><!-- /.menu-title -->
                <li><a href="#" onclick="dashboardModule.get_unset_records();"><i class="menu-icon fa fa-square-o"></i>Unset Records</a></li>
                <li><a href="#" onclick="dashboardModule.get_null_records();"><i class="menu-icon fa fa-circle-o"></i>No OCLC Numbers</a></li>
                <li><a href="#" onclick="dashboardModule.get_not_found_records();"><i class="menu-icon fa fa-ban"></i>Not Found in WorldCat</a></li>
                <li><a href="/dashboard/users"><i class="menu-icon fa fa-users"></i>Dashboard Users</a></li>

            </ul>
        </div><!-- /.navbar-collapse -->
    </nav>
</aside>
<!-- /#left-panel -->
<!-- Right Panel -->
<div id="right-panel" class="right-panel">
    <!-- Header-->
    <header id="header" class="header">
        <div class="top-left">
            <div class="navbar-header">
                <a class="navbar-brand" href="https://library.du.edu" target="_blank"><img src="../images/du-logo.png" alt="Logo"></a>
                <a id="menuToggle" class="menutoggle"><i class="fa fa-bars"></i></a>
            </div>
        </div>
        <div class="top-right">
            <div class="header-menu">
                <div class="user-area dropdown float-right"></div>
            </div>
        </div>
    </header>
    <!-- /#header -->
    <!-- Content -->
    <div class="content">
        <div class="animated fadeIn">
            <div class="row">
                <div class="col-lg-2 col-md-6"> <!-- col-lg-3 col-md-6 -->
                    <div class="card">
                        <div class="card-body">
                            <div class="stat-widget-one">
                                <!--
                                <div class="stat-icon dib"><i class="ti-server text-primary"></i></div>
                                -->
                                <div class="stat-content dib">
                                    <div class="stat-text"><i class="ti-server text-primary"></i>&nbsp;&nbsp;Total Records</div>
                                    <div class="total-cataloged-records">Loading...</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 col-md-6">
                    <div class="card">
                        <div class="card-body">
                            <div class="stat-widget-one">
                                <!--
                                <div class="stat-icon dib"><i class="fa fa-square-o"></i></div>
                                -->
                                <div class="stat-content dib">
                                    <div class="stat-text"><i class="fa fa-square-o"></i>&nbsp;&nbsp;Unset Records</div>
                                    <div class="total-unset-records">Loading...</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 col-md-6">
                    <div class="card">
                        <div class="card-body">
                            <div class="stat-widget-one">
                                <!--
                                <div class="stat-icon dib"><i class="fa fa-square"></i></div>
                                -->
                                <div class="stat-content dib">
                                    <div class="stat-text"><i class="fa fa-square"></i>&nbsp;&nbsp;Set Records</div>
                                    <div class="total-set-records">Loading...</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="card">
                        <div class="card-body">
                            <div class="stat-widget-one">
                                <!--
                                <div class="stat-icon dib"><i class="fa fa-circle-o"></i></div>
                                -->
                                <div class="stat-content dib">
                                    <div class="stat-text"><i class="fa fa-circle-o"></i>&nbsp;&nbsp;No OCLC Numbers</div>
                                    <div class="total-null-records">Loading...</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="card">
                        <div class="card-body">
                            <div class="stat-widget-one">
                                <!--
                                <div class="stat-icon dib"><i class="fa fa-circle-o"></i></div>
                                -->
                                <div class="stat-content dib">
                                    <div class="stat-text"><i class="fa fa-ban"></i>&nbsp;&nbsp;Not found in WorldCat</div>
                                    <div class="total-notfound-records">Loading...</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>

            <div class="row"><div id="message"></div></div>

            <div class="row" id="reclamation-data">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title"></strong>
                        </div>
                        <div class="card-body">
                            <table id="data-table" class="table table-striped table-bordered"></table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /.content -->
    <div class="clearfix"></div>
    <!-- Footer -->
    <%- include('partials/footer.ejs'); %>
    <!-- /.site-footer -->
</div>
